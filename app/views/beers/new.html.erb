<h1>Create Beer</h1>
<%= form_tag "/beers", method:  :post do %>
  <div>
  <%= label_tag :name %>
  <%= text_field_tag :name %>
  </div>
  <div>
  <%= label_tag :style %>
  <%= text_field_tag :style %>
  </div>
  <div>
  <%= label_tag :ibu %>
  <%= text_field_tag :ibu %>
  </div>
  <div>
  <%= label_tag :alcohol %>
  <%= text_field_tag :alcohol %>
  </div>
  <%= submit_tag "Submit" %>
<% end %>